{
    "name": "kshitiz-lang-extention",
    "displayName": "Kshitiz-lang extention",
    "description": "Syntax hilighting for kshitiz lang",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],

    "icon": "https:logo/Kshitiz.png",
    "galleryBanner": {
        "color": "#c80000",
        "theme": "dark"
    },
    "contributes": {
        "languages": [{
            "id": "ksh",
            "aliases": ["Kshitiz", "ksh"],
            "extensions": [".ksh"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ksh",
            "scopeName": "source.ksh",
            "path": "./syntaxes/ksh.tmLanguage.json"
        }]
    }
}